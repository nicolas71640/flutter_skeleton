{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Frontend: Launch App",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "toolArgs": [
                "-v",
                "--dart-define=FIREBASE_ENABLED=false",
                "--dart-define=BASE_URL=http://192.168.43.199:3000/api"
            ]
        },
        {
            "name": "Frontend: Run Unit Tests",
            "type": "dart",
            "request": "launch",
            "program": "./test/",
            "args": [
                "--coverage",
                "--dart-define=FIREBASE_ENABLED=false",
                "--dart-define=BASE_URL=http://192.168.43.199:3000/api"
            ]
        },
        {
            "name": "Frontend: Run Integration Tests",
            "type": "dart",
            "request": "launch",
            "program": "./integration_test/",
            "toolArgs": [
                "-v",
                "--dart-define=FIREBASE_ENABLED=false",
                "--dart-define=BASE_URL=http://192.168.43.199:3000/api"
            ]
        },
        {
            "name": "Run",
            "type": "dart",
            "request": "launch",
            "program": "./integration_test/",
            "codeLens": {
                "path": "integration_test",
                "for": [
                    "run-test",
                    "run-test-file"
                ]
            },
            "toolArgs": [
                "-v",
                "--dart-define=FIREBASE_ENABLED=false",
                "--dart-define=BASE_URL=http://192.168.43.199:3000/api"
            ]
        },
        {
            "name": "Debug",
            "type": "dart",
            "request": "launch",
            "program": "./integration_test/",
            "codeLens": {
                "path": "integration_test",
                "for": [
                    "debug-test",
                    "debug-test-file"
                ]
            },
            "toolArgs": [
                "-v",
                "--dart-define=FIREBASE_ENABLED=false",
                "--dart-define=BASE_URL=http://192.168.43.199:3000/api"
            ]
        },
        {
            "name": "Run for Manual",
            "type": "dart",
            "request": "launch",
            "program": "./integration_test/",
            "codeLens": {
                "path": "manual_test/",
                "for": [ "run-test", "run-test-file", "debug-test", "debug-test-file" ],

            },
            "toolArgs": [
                "-v",
                "--dart-define=FIREBASE_ENABLED=true",
                "--dart-define=BASE_URL=http://192.168.43.199:3000/api"
            ]
        },
    ]
}